<manifest 
android:versionName="0.1" android:versionCode="1" package="de.sciss.android.synth" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="7"></uses-sdk>
    <application android:icon="@drawable/app_icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /><!-- do we need this? XXX -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
</manifest>
